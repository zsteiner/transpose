{
  "name": "transpose",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "browerslist": [
    "defaults and fully supports es6-module",
    "maintained node versions"
  ],
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "lint": "npm run lint:eslint && yarn run lint:style",
    "lint:fix": "npm run lint:eslint && yarn run lint:style:fix",
    "lint:eslint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore --fix src",
    "lint:style": "stylelint 'src/**/*.{css,vue}'",
    "lint:style:fix": "npm run lint:style -- --fix"
  },
  "dependencies": {
    "vue": "^3.4.26",
    "vue-router": "^4.3.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "abcjs": "^6.3.0",
    "eslint": "8",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "9",
    "eslint-plugin-vuejs-accessibility": "^2.3.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.5.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-svg-loader": "^5.1.0"
  }
}